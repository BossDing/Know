<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.jude.know">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".app.APP"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">
        <activity
            android:name=".module.question.MainActivity"
            android:label="Know">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".module.question.WriteQuestionActivity"
            android:label="提问"></activity>
        <activity android:name=".module.question.AnswerActivity"
            android:label="问题"></activity>
    </application>

</manifest>
